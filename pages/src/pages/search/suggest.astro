---
import { Swell } from '@swell/storefrontjs';
import { PredictiveSearchResource } from '@/resources/predictive-search';
import ThemePage from '../../components/ThemePage.astro';

const query = Astro.url.searchParams.get('q');
---

<ThemePage
  id="search"
  getData={(swell: Swell) => ({
    predictive_search: new PredictiveSearchResource(swell, query),
  })}
/>
