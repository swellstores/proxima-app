---
import { Swell } from '@/swell/api';
import ThemePage from '@/components/ThemePage.astro';
import { getCategories } from '@/swell/categories';
---

<ThemePage
  id="categories/index"
  getData={async (swell: Swell) => {
    const categories = await getCategories(swell);

    return {
      categories,
    };
  }}
/>
