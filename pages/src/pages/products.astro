---
import Layout from '../layouts/Layout.astro';
import { setSwellCredentials } from "../swell/api";
import { getProducts } from "../swell/products";

await setSwellCredentials(Astro);
const products = await getProducts();
---

<Layout title="Swell Storefront App (Astro)">
	<pre>{JSON.stringify(products, null, 2)}</pre>
</Layout>
