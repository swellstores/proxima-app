---
import { Swell } from '@swell/storefrontjs';
import { AccountOrderResource } from '@/resources/account';
import ThemePage from '@/components/ThemePage.astro';

const { id } = Astro.params;
---

<ThemePage
  id="account/order"
  required="order.id"
  getData={(swell: Swell) => ({
    order: new AccountOrderResource(swell, id as string),
  })}
/>
